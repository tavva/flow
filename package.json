{
  "name": "flow",
  "version": "0.1.0",
  "description": "GTD coach for Flow-based Obsidian vaults",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "lint": "tsc -noEmit -skipLibCheck",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "evaluate": "ts-node -P evaluation/tsconfig.json evaluation/run-evaluation.ts",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "cli": "node dist/cli.mjs",
    "build:cli": "node esbuild.cli.mjs"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "gtd",
    "flow",
    "productivity"
  ],
  "author": "Ben",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^20.11.19",
    "@types/react": "^18.3.26",
    "@types/react-test-renderer": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.25.0",
    "ink-testing-library": "^4.0.0",
    "jest": "^29.7.0",
    "obsidian": "latest",
    "obsidian-dataview": "^0.5.68",
    "prettier": "^3.6.2",
    "react-test-renderer": "^18.3.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "ink": "^5.2.1",
    "ink-spinner": "^5.0.0",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "react": "^18.3.1"
  }
}
