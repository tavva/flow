{
	"name": "Flow",
	"version": "0.6.5",
	"description": "Flow, inspired by David Allen's Getting Things Done (GTD), is a way of managing everything in your life so you focus on what's important.",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"test": "NODE_OPTIONS='$NODE_OPTIONS --experimental-vm-modules' npx jest",
		"format": "prettier --write \"src/**/*.{ts,js,json,svelte}\""
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@tsconfig/svelte": "^5.0.4",
		"@types/fs-extra": "^11.0.4",
		"@types/jest": "^29.5.12",
		"@types/lodash": "^4.17.4",
		"@types/luxon": "^3.4.2",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "^0.21.3",
		"esbuild-sass-plugin": "^3.3.0",
		"esbuild-svelte": "^0.8.0",
		"generate-license-file": "^3.5.0",
		"jest": "^29.7.0",
		"obsidian": "^1.7.2",
		"obsidian-dataview": "^0.5.66",
		"prettier-plugin-svelte": "^3.3.3",
		"sass": "^1.77.2",
		"svelte": "^4.2.19",
		"svelte-preprocess": "^5.1.4",
		"ts-jest": "^29.1.5",
		"ts-node": "^10.9.2",
		"tslib": "2.4.0",
		"typescript": "^5.5.3"
	},
	"prettier": {
		"semi": false,
		"singleQuote": true,
		"overrides": [
			{
				"files": "*.svelte",
				"options": {
					"parser": "svelte"
				}
			}
		]
	},
	"dependencies": {
		"dotenv": "^16.4.5",
		"fs-extra": "^11.2.0",
		"lodash": "^4.17.21",
		"luxon": "^3.4.4"
	}
}
